{
  "manifest_version": "2",
  "api_version": "1",
  "name": "youdao translate extension",
  "description": "youdao Translate Extension Description",
  "developer_name": "ccphamy",
  "icon": "images/icon.png",
  "options": {
    "query_debounce": 0.1
  },
  "preferences": [
    {
      "id": "translate_kw",
      "type": "keyword",
      "name": "tr",
      "description": "中文=>zh-CHS|英文=>en|日文=>ja|韩文=>ko|法文=>fr|西班牙文=>es|葡萄牙文=>pt|意大利文=>it|俄文=>ru|越南文=>vi|德文=>de|阿拉伯文=>ar|印尼文=>id",
      "default_value": "tr"
    },
    {
      "id": "source",
      "type": "select",
      "name": "Source Language",
      "description": "Source Language,Default:auto",
      "default_value": "auto",
      "options": [
        "auto",
        "zh-CHS",
        "en",
        "ja",
        "ko",
        "fr",
        "es",
        "pt",
        "it",
        "vi",
        "de",
        "ar",
        "id"
      ]
    },
    {
      "id": "to",
      "type": "select",
      "name": "To Language",
      "description": "To Language,Default:ja",
      "default_value": "ja",
      "options": [
        "zh-CHS",
        "en",
        "ja",
        "ko",
        "fr",
        "es",
        "pt",
        "it",
        "vi",
        "de",
        "ar",
        "id"
      ]
    },
    {
      "id": "repairTo",
      "type": "select",
      "name": "To Language",
      "description": "To Language,Default:zh-CHS",
      "default_value": "ja",
      "options": [
        "zh-CHS",
        "en",
        "ja",
        "ko",
        "fr",
        "es",
        "pt",
        "it",
        "vi",
        "de",
        "ar",
        "id"
      ]
    }
  ]
}